<html>
    <body>
        <%- include("../partials/navbar") %>
        <h1> Plantas </h1>
        <table>
            <tr>
                <td>id</td>
                <td>nombre</td>
                <td>ciudad</td>
                <td>estado</td>
            </tr>
            <% for(let i in list) { %>
                <tr> 
                    <td><%= list[i].id %> </td>
                    <td><%= list[i].nombre %> </td>
                    <td><%= list[i].ciudad %> </td>
                    <td><%= list[i].estado %></td>
                </tr>
            <% } %>
        </table>

        <a href="./planta/nuevo">Crear nuevo</a>
        

    </body>
</html>