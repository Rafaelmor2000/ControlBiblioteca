<html>
    
    <%- include("../partials/head") %>

    <body>
        <%- include("../partials/navbar") %>

        <h1> Edificios </h1>
        <table  class="table table-bordered">
            <thead class="table-dark">
            <tr>
                <th>id</th>
                <th>Nombre</th>
                <th>Edificio</th>
                <th>Planta</th>
            </tr>
            </thead>
            <tbody>
            <% for(let i in list) { %>
                <tr> 
                    <th><%= list[i].id %> </th>
                    <td><%= list[i].nombre %> </td>
                    <td><%= list[i].edificio %></td>
                    <td><%= list[i].planta %> </td>
                </tr>
            <% } %>
            </tbody>
        </table>

        <a href="./zona/nuevo">Crear nuevo</a>
        

    </body>
</html>